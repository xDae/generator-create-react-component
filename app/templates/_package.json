  "module": "es/index.js",
  "author": "<%= author %>",
  "homepage": "https://github.com/<%= ghUser %>/<%= name %>",
  "license": "<%= license %>",
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= ghUser %>/<%= name %>.git"
